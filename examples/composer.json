{
    "name": "opentelemetry/wordpress-example",
    "description": "An example of autoinstrumentation of wordpress with the official wordpress docker image",
    "type": "project",
    "require": {
	"instana/opentelemetry-php-exporter": "dev-main",
	"open-telemetry/api":"1.2.0",
	"open-telemetry/opentelemetry-propagation-instana": "0.1.0",
        "open-telemetry/opentelemetry-auto-wordpress": "0.0.15",
        "open-telemetry/transport-grpc": "1.0.0",
        "open-telemetry/sdk": "1.1.1",
        "php-http/guzzle7-adapter": "1.0.0",
	"open-telemetry/opentelemetry-auto-curl": "0.0.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
